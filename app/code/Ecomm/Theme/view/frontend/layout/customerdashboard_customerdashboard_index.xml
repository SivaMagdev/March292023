		<referenceContainer name="content" htmlTag="div" htmlClass="page-main">
			<block class="Ecomm\Theme\Block\CustomerDashboard" name="customer.dashboard.content" template="Ecomm_Theme::CustomerDashboard/content.phtml" cacheable="false"/>
		</referenceContainer>
	</body>
</page>
