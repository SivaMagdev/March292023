	<body>
		<referenceContainer name="main.content" htmlTag="main" htmlClass="page-main-full-width">
			<block class="Ecomm\Theme\Block\CustomerDashboard" name="customer.dashboard" before="-" template="Ecomm_Theme::CustomerDashboard/index.phtml" cacheable="false"/>
